FRAMEWORK_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/PXGradientView-iOS10.2" "$PODS_CONFIGURATION_BUILD_DIR/UIColor-MoreColors-iOS10.2"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$PODS_CONFIGURATION_BUILD_DIR/PXGradientView-iOS10.2/PXGradientView.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/UIColor-MoreColors-iOS10.2/UIColor_MoreColors.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "PXGradientView" -framework "UIColor_MoreColors"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
